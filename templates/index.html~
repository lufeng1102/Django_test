function search(el)
{
var key=$("#searchkey").val();
var ac=$("#actionType").val();

if(key=="请输入关键词"||key=="")
{
alert("请输入关键词");
$("#searchkey")[0].focus();
return false;
}

searchAgent.init(51, ac);
searchAgent.Search(encodeURIComponent(key));
el.enable=false;
}
